{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody class="table-group-divider">
    {{#articles}}
        <tr><a href="/articles/{{id}}">
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </a></tr>
    {{/articles}}
    </tbody>
</table>
<a href="/articles/new" class="btn btn-secondary">글쓰기</a>
{{>layouts/footer}}